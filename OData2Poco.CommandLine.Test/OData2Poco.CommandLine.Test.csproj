<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.1;net452</TargetFrameworks>

    <Authors>Mohamed Hassan</Authors>
    <AssemblyTitle>OData2Poco.Tests</AssemblyTitle>
    <Product>OData2Poco</Product>
    <IsTestProject>true</IsTestProject>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="ProgramTests.NetCore.cs.txt" />
  </ItemGroup>
  


  <ItemGroup>
    <Compile Include="..\CommonTestData.cs" Link="CommonTestData.cs" />
  </ItemGroup>

  <PropertyGroup Condition=" '$(TargetFramework)' == 'net452'">
    <DefineConstants>NET45;NETFULL</DefineConstants>
  </PropertyGroup>

 

  <ItemGroup>
    <ProjectReference Include="..\OData2Poco.CommandLine\OData2Poco.CommandLine.csproj" />
  </ItemGroup>


  <ItemGroup>
    <None Update="Data\invalidxml.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Data\northwindV3.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Data\northwindV4.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>


  
</Project>
