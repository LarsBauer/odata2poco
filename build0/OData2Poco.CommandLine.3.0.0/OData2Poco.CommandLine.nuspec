<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>OData2Poco.CommandLine</id>
    <version>3.0.0</version>
    <authors>Mohamed Hassan</authors>
    <owners>Mohamed Hassan</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://github.com/moh-hassan/odata2poco/blob/master/license.txt?raw=true</licenseUrl>
    <projectUrl>https://github.com/moh-hassan/odata2poco</projectUrl>
    <iconUrl>https://github.com/moh-hassan/odata2poco/blob/master/ODataLogo-32.ico?raw=true</iconUrl>
    <description>o2gen is a CommandLine tool for generating c# POCO classes from OData feeds.
      Project site: https://github.com/moh-hassan/odata2poco</description>
    <releaseNotes>Version: 3.0.0
      What is new:
      - Maintainance: Migrating to vs2017 and c#7
      - New: Support colored Console
      - New: Support NetStandard2.0,NetStandard1.1 and net45</releaseNotes>
    <copyright>Copyright ©  Mohamed Hassan 2016</copyright>
    <tags>OData poco code-generation</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.5">
        <dependency id="OData2Poco" version="3.0.0" exclude="Build,Analyzers" />
        <dependency id="CommandLineParser" version="2.3.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETCoreApp2.1">
        <dependency id="OData2Poco" version="3.0.0" exclude="Build,Analyzers" />
        <dependency id="CommandLineParser" version="2.4.3" exclude="Build,Analyzers" />
        <dependency id="Microsoft.NETCore.App" version="2.1.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>